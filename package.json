{
  "name": "react-charts",
  "version": "0.0.7",
  "description": "Simple, immersive & interactive charts for React",
  "license": "MIT",
  "homepage": "https://github.com/react-charts/react-charts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-charts/react-charts.git"
  },
  "keywords": [
    "react",
    "form",
    "forms",
    "form builder",
    "nested forms"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/",
    "react-charts.js",
    "react-charts.css",
    "media/*.png"
  ],
  "scripts": {
    "build:node": "babel src --out-dir lib --source-maps inline",
    "watch": "onchange 'src/**' -i -- npm-run-all build:*",
    "test": "standard",
    "umd": "rimraf react-move.js && webpack --config umd.webpack.js",
    "umd:perf": "rimraf react-move.js && webpack --config umd-perf.webpack.js",
    "prepublish": "npm-run-all build:* && npm run umd",
    "postpublish": "git push --tags",
    "docs": "yarn watch & cd docs && yarn && yarn start",
    "docs:build": "cd docs && yarn && yarn run build",
    "preview": "yarn docs:build && serve -s docs/build"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3-quadtree": "^1.0.3",
    "d3-scale": "^1.0.6",
    "d3-shape": "^1.2.0",
    "d3-voronoi": "^1.1.2",
    "javascript-detect-element-resize": "^0.5.3",
    "raf": "^3.3.2",
    "react-move": "^1.5.1",
    "react-state": "^2.0.1"
  },
  "peerDependencies": {
    "react": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.3.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "react": "^15.5.4",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1"
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "parser": "babel-eslint",
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ]
  }
}
